{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "strictFunctionTypes": false,
    "target": "esnext",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./../*"]
    },
    "types": [
      "@nuxt/vue-app",
      "@types/node",
      "@types/jest"
    ]
  }
}

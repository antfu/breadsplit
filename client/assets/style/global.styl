body, html, #app, #__nuxt, #__layout
  height 100%
  overflow hidden !important

::-webkit-scrollbar
  width 5px

::-webkit-scrollbar-track
  background rgba(0,0,0,0.05)

::-webkit-scrollbar-thumb
  background rgba(0,0,0,0.1)


@media only screen and (min-width: 1904px)
  .container {
      max-width: 1185px;
  }

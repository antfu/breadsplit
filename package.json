{
  "name": "breadsplit",
  "fullname": "BreadSplit",
  "version": "0.4.0",
  "private": true,
  "description": "The BreadSplit web application",
  "author": "The BreadSplit Team <https://github.com/breadsplit/breadsplit>",
  "keywords": [
    "accounting",
    "opensource",
    "expenses",
    "split",
    "splitbill",
    "breadsplit",
    "money",
    "vue",
    "pwa",
    "shared-expenses"
  ],
  "license": "GPLv3",
  "repository": {
    "type": "git",
    "url": "https://github.com/breadsplit/breadsplit"
  },
  "bugs": "https://github.com/breadsplit/breadsplit/issues",
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 5.0.0"
  },
  "scripts": {
    "dev": "nuxt ./client",
    "generate": "cross-env NODE_ENV=production nuxt generate ./client",
    "generate:host": "static-server ./client/dist -p 3000",
    "web:dev": "nuxt ./website",
    "web:generate": "cross-env NODE_ENV=production nuxt generate ./website",
    "start": "cross-env NODE_ENV=production nuxt start ./client",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "deploy:firebase": "node scripts/firebase-deploy.js",
    "electron:install": "npm i --prefix=./electron",
    "electron:dev": "npm run dev --prefix=./electron",
    "electron:pack": "npm run build --prefix=./electron",
    "electron:build": "cross-env BUILD_TARGET=electron npm run generate && npm run electron:pack",
    "cy:install": "npm i -g cypress",
    "cy:run:ci": "cypress run --record",
    "cy:open": "cypress open",
    "cy:dev": "concurrently \"npm run dev\" \"npm run cy:open\"",
    "docs:dev": "npx vuepress dev docs",
    "docs:build": "npx vuepress build docs",
    "test": "jest",
    "test:firebase": "jest --config=./firebase/functions/jest.config.js",
    "test:watch": "jest --watch",
    "test:e2e": "start-server-and-test generate:host http://localhost:3000 cy:run:ci"
  },
  "dependencies": {
    "@dicebear/avatars": "^3.0.6",
    "@dicebear/avatars-jdenticon-sprites": "^1.0.3",
    "@nuxtjs/google-gtag": "^1.0.4",
    "@nuxtjs/pwa": "^3.0.0-beta.14",
    "@nuxtjs/sentry": "^2.3.2",
    "axios": "^0.19.0",
    "blueimp-md5": "^2.10.0",
    "d3": "^5.9.2",
    "dayjs": "^1.8.12",
    "express": "^4.16.4",
    "firebase": "^5.10.1",
    "jdenticon": "^2.1.1",
    "json-stable-stringify": "^1.0.1",
    "lodash": "^4.17.11",
    "nanoid": "^2.0.1",
    "nuxt": "^2.6.3",
    "nuxt-property-decorator": "^2.1.3",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "rusha": "^0.8.13",
    "svg-to-dataurl": "^1.0.0",
    "ts-node": "^8.1.0",
    "ts-optchain": "^0.1.7",
    "typescript": "^3.4.3",
    "vue-class-component": "^7.1.0",
    "vue-clipboard2": "^0.3.0",
    "vue-i18n": "^8.10.0",
    "vuetify": "^1.5.12",
    "vuetify-loader": "^1.2.2",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.0.3",
    "@nuxt/typescript": "^2.6.2",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@types/blueimp-md5": "^2.7.0",
    "@types/d3": "^5.7.2",
    "@types/fs-extra": "^7.0.0",
    "@types/jest": "^24.0.11",
    "@types/json-stable-stringify": "^1.0.32",
    "@types/lodash": "^4.14.123",
    "@types/nanoid": "^1.2.1",
    "@types/webpack-env": "^1.13.9",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.4.1",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.3",
    "eslint-plugin-promise": ">=4.1.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "firebase-admin": "^7.3.0",
    "firebase-functions": "^2.3.0",
    "firebase-functions-test": "^0.1.6",
    "firebase-tools": "^6.9.0",
    "fs-extra": "^8.0.1",
    "jest": "^24.7.1",
    "lerna": "^3.14.1",
    "nodemon": "^1.18.11",
    "replace-in-file": "^4.0.0",
    "rimraf": "^2.6.3",
    "shelljs": "^0.8.3",
    "start-server-and-test": "^1.9.0",
    "static-server": "^2.2.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^24.0.2",
    "vue-jest": "^3.0.4"
  },
  "optionalDependencies": {
    "cypress": "^3.3.1"
  }
}

{
  "name": "splitoast",
  "fullname": "Splitoast",
  "version": "0.1.1",
  "private": true,
  "description": "The Splitoast web application",
  "author": "Anthony Fu",
  "scripts": {
    "dev": "nuxt ./client",
    "build": "nuxt build ./client",
    "generate": "cross-env NODE_ENV=production nuxt generate ./client",
    "start": "cross-env NODE_ENV=production nuxt start ./client",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "electron:install": "npm run i --prefix=./electron",
    "electron:dev": "cross-env BUILD_TARGET=electron npm run dev --prefix=./electron",
    "electron:pack": "npm run build --prefix=./electron",
    "electron:build": "cross-env BUILD_TARGET=electron npm run generate && npm run electron:pack",
    "cordova:install": "npm run i --prefix=./cordova",
    "cordova:check": "npm run check --prefix=./cordova",
    "cordova:generate": "npm run generate && npm run cordova:copy",
    "cordova:copy": "rimraf ./cordova/www && shx cp -R ./client/dist ./cordova/www && replace-in-file //nuxt//g nuxt/ ./cordova/www/**/* --quiet --isRegex",
    "cordova:prepare": "npm run prepare --prefix=./cordova",
    "cordova:build": "npm run build --prefix=./cordova",
    "cordova:run:android": "npm run android:run --prefix=./cordova",
    "cordova": "cross-env BUILD_TARGET=android npm run cordova:generate && npm run cordova:build && npm run cordova:run:android",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "bash scripts/docs-deploy.sh",
    "test": "jest --config=./client/jest.config.js",
    "test:watch": "jest --config=./client/jest.config.js --watch --coverage"
  },
  "dependencies": {
    "@dicebear/avatars": "^3.0.6",
    "@dicebear/avatars-gridy-sprites": "^1.0.11",
    "@mdi/font": "^3.5.95",
    "@nuxtjs/axios": "^5.4.1",
    "@nuxtjs/google-gtag": "^1.0.4",
    "@nuxtjs/pwa": "^3.0.0-beta.14",
    "chroma-js": "^2.0.3",
    "crypto-js": "^3.1.9-1",
    "dayjs": "^1.8.12",
    "express": "^4.16.4",
    "firebase": "^5.10.1",
    "lodash": "^4.17.11",
    "nuxt": "^2.6.2",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "svg-to-dataurl": "^1.0.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.3",
    "vue-i18n": "^8.10.0",
    "vue-property-decorator": "^8.1.0",
    "vue-swatches": "^1.0.3",
    "vuetify": "^1.5.12",
    "vuetify-loader": "^1.2.2",
    "vuex-class": "^0.3.2",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@nuxt/typescript": "^2.6.2",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/webpack-env": "^1.13.9",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": ">=12.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": ">=2.16.0",
    "eslint-plugin-jest": ">=22.4.1",
    "eslint-plugin-node": ">=8.0.1",
    "eslint-plugin-nuxt": ">=0.4.3",
    "eslint-plugin-promise": ">=4.1.1",
    "eslint-plugin-standard": ">=4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.7.1",
    "nodemon": "^1.18.11",
    "replace-in-file": "^4.0.0",
    "rimraf": "^2.6.3",
    "shx": "^0.3.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^24.0.2",
    "vue-jest": "^3.0.4",
    "vuepress": "^0.14.11"
  }
}

{
  "name": "breadsplit",
  "fullname": "BreadSplit",
  "version": "0.5.0",
  "private": true,
  "description": "The BreadSplit web application",
  "author": "The BreadSplit Team <https://github.com/breadsplit/breadsplit>",
  "keywords": [
    "accounting",
    "opensource",
    "expenses",
    "split",
    "splitbill",
    "breadsplit",
    "money",
    "vue",
    "pwa",
    "shared-expenses"
  ],
  "license": "GPLv3",
  "repository": {
    "type": "git",
    "url": "https://github.com/breadsplit/breadsplit"
  },
  "homepage": "https://breadsplit.com",
  "bugs": "https://github.com/breadsplit/breadsplit/issues",
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 5.0.0"
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "‚ö° Features"
      },
      {
        "type": "fix",
        "section": "üêû Bug Fixes"
      },
      {
        "type": "i18n",
        "section": "üåç Internationalization"
      },
      {
        "type": "ui",
        "section": "üì± User interface"
      },
      {
        "type": "test",
        "section": "‚öó Tests"
      },
      {
        "type": "docs",
        "section": "üìö Documentations"
      },
      {
        "type": "ci",
        "hidden": true
      }
    ]
  },
  "scripts": {
    "dev": "nuxt ./client",
    "generate": "cross-env NODE_ENV=production nuxt generate ./client",
    "generate:host": "static-server ./client/dist -p 3000",
    "web:dev": "nuxt ./website",
    "web:generate": "cross-env NODE_ENV=production nuxt generate ./website",
    "start": "cross-env NODE_ENV=production nuxt start ./client",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:fix": "eslint --fix --ext .ts,.js,.vue --ignore-path .gitignore .",
    "deploy:firebase": "node scripts/firebase-deploy.js",
    "electron:install": "npm i --prefix=./electron",
    "electron:dev": "npm run dev --prefix=./electron",
    "electron:pack": "npm run build --prefix=./electron",
    "electron:build": "cross-env BUILD_TARGET=electron npm run generate && npm run electron:pack",
    "cy:install": "npm i -g cypress",
    "cy:run:ci": "cypress run --record",
    "cy:open": "cypress open",
    "cy:dev": "concurrently \"npm run dev\" \"npm run cy:open\"",
    "docs:dev": "npx vuepress dev docs",
    "docs:build": "npx vuepress build docs",
    "test": "jest",
    "test:firebase": "jest --config=./firebase/functions/jest.config.js",
    "test:watch": "jest --watch",
    "test:e2e": "start-server-and-test generate:host http://localhost:3000 cy:run:ci",
    "release": "standard-version"
  },

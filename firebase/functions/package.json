    "firebase-admin": "8.0.0",
    "firebase-functions": "2.3.1",
    "js-cache": "1.0.3",
    "json-stable-stringify": "1.0.1",
    "lodash": "4.17.11",
    "nanoid": "2.0.3",
    "rusha": "0.8.13"
  },
  "devDependencies": {
    "firebase-tools": "6.11.0",
    "typescript": "3.5.1",
    "webpack": "4.33.0",
    "webpack-cli": "3.3.3",
    "webpack-node-externals": "1.7.2"
  },
  "version": "0.4.0"
